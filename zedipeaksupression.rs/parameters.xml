<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="numberofhvacs" displayName="Number of HVACS" type="int" 
						defaultValue="24" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="numberofbuildings" displayName="Number of buildings" type="int" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="numberofdays" displayName="Number of days monitored" type="int" 
						defaultValue="7" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		<parameter name="alpha" displayName="Peak load coefficient" type="double" 
						defaultValue="0.8" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
		<parameter name="timeInterval" displayName="Time Interval for shifting" type="int" 
						defaultValue="6" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		<parameter name="heatTimeInterval" displayName="Time Interval for shifting" type="int" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		<parameter name="avgDegree" displayName="Avg number of neighbours" type="int" 
						defaultValue="4" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="endDate" displayName="endDate" type="java.lang.String" 
						defaultValue="08/01/2018 00:00" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									
		/>
				<parameter name="netName" displayName="Network Name" type="java.lang.String" 
						defaultValue="CM" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									values="CM WS1 Random Partition RandomSparse RandomDense WS "
						
		/>
				<parameter name="numberofflats" displayName="Number of Flats in the building" type="int" 
						defaultValue="100" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="startDate" displayName="startDate" type="java.lang.String" 
						defaultValue="01/01/2018 00:00" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									
		/>
		</parameters>


